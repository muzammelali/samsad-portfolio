---
import Footer from '../components/Footer.astro';
import Hero from '../components/Hero.astro';
import JourneySection from '../components/JourneySection.astro';
import Navbar from '../components/Navbar.astro';
import ProjectDetailsSection from '../components/ProjectDetailsSection.astro';
import ProjectIdea from '../components/ProjectIdea.astro';
import UikitSection from '../components/Uikitsection.astro';
import WorkWith from '../components/WorkWith.astro';
import Layout from '../layouts/Layout.astro';

---

<Layout>
	<main class="py-5 md:py-10 container">
		<Navbar />
		<Hero></Hero>
		<div class="pt-[70px] md:pt-24"></div>
		<ProjectDetailsSection />
		<div class="py-6"></div>
		<UikitSection />
		<div class="py-6"></div>
		<JourneySection />
		<div class="pt-20 md:pt-[100px]"></div>
		<WorkWith />
		<div class="pt-20 md:pt-[100px]"></div>
		<ProjectIdea />
		<div class="pt-12 md:pt-[60px]"></div>
		<Footer />
	</main>
</Layout>

